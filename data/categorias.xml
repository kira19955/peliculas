<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!--SE CREO DATA PARA EL MODELO DE RES.CATEGORY-->
        <record id="category_director" model="res.partner.category">
            <field name="name">Director</field>
        </record>

        <record id="category_actor" model="res.partner.category">
            <field name="name">Actor</field>
        </record>

        <!--DATA CREADA PARA LAS SECUENCIAS-->
        <record id="sequence_peliculas" model="ir.sequence">
            <field name="name">Secuencia para presupuesto de peliculas</field>
            <field name="implementation">no_gap</field>
            <field name="code">secuencia.presupuesto.peliculas</field>
            <field name="prefix">PP</field>
            <field name="suffix">-%(year)s</field>
            <field name="padding">3</field>
            <field name="number_increment">1</field>
        </record>
    </data>
</odoo>